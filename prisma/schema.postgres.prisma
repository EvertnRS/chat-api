generator client_postgres {
  provider = "prisma-client-js"
  output   = "../src/infra/database/generated/client-postgres"
}

datasource postgres {
  provider  = "postgresql"
  url  	    = env("POSTGRES_DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
}